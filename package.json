{
  "name": "konfig",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode client && cp -f ./dist/.vite/manifest.json ./src/lib/manifest.json && vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "$npm_execpath run build && wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "test": "vitest run --coverage",
    "prettier": "prettier --config prettier.config.mjs '{app,src,components,lib}/**/*.{ts,tsx,js,css}' --write"
  },
  "dependencies": {
    "@heroicons/react": "2.2.0",
    "@hono/react-renderer": "1.0.1",
    "@hookform/resolvers": "5.2.2",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-menubar": "1.1.16",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tabs": "1.1.13",
    "@tailwindcss/vite": "4.1.16",
    "class-variance-authority": "0.7.1",
    "cmdk": "1.1.1",
    "hono": "4.10.3",
    "lucide-react": "0.548.0",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.65.0",
    "sonner": "2.0.7",
    "swr": "2.3.6",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.16",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.13.15",
    "@cloudflare/vitest-pool-workers": "0.10.0",
    "@cloudflare/workers-types": "4.20251014.0",
    "@eslint/js": "9.38.0",
    "@hono/vite-build": "1.7.0",
    "@hono/vite-dev-server": "0.23.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "@vitest/coverage-istanbul": "3.2.4",
    "eslint": "9.38.0",
    "eslint-plugin-react": "7.37.5",
    "jsdom": "27.0.1",
    "msw": "2.11.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.12",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wrangler": "4.45.0"
  }
}
