{
  "extends": [
    "config:best-practices",
    ":pinDependencies",
    ":disableDependencyDashboard",
    ":automergeMinor"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "next-on-pages"
      ],
      "groupName": "next-on-pages"
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "digest": {
        "automerge": true
      }
    }
  ],
  "postUpdateOptions": [
    "npmDedupe"
  ]
}
