{
  "name": "konfig",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run --coverage",
    "prettier": "prettier --config .prettierrc 'app/**/*.{ts,tsx,js,css}' --write",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2023-12-01 --compatibility-flag=nodejs_compat"
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "1.8.5",
    "@cloudflare/workers-types": "4.20240117.0",
    "@miniflare/shared-test-environment": "2.14.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "20.11.7",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitest/coverage-v8": "1.2.2",
    "autoprefixer": "10.4.17",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-next-on-pages": "1.8.5",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3",
    "vercel": "33.3.0",
    "vitest": "1.2.2",
    "vitest-environment-miniflare": "2.14.2",
    "wrangler": "3.24.0"
  }
}
