    "clsx": "2.1.0",
    "next": "14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.51.1",
    "swr": "2.2.5",
    "tailwind-merge": "2.2.2",
