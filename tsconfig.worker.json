{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.worker.tsbuildinfo",
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "node",
    "types": ["@cloudflare/workers-types", "@cloudflare/vitest-pool-workers"],
    "paths": {
      "@/*": ["./src/*"],
      "@functions/*": ["./functions/*"]
    },
    "outDir": "./node_modules/.tmp/functions"
  },
  "include": ["functions"]
}
